package net.sf.gridarta.model.archetypetype;

import org.jetbrains.annotations.NotNull;

/**
 * Interface for visitors of {@link ArchetypeAttribute ArchetypeAttributes}.
 * @author Andreas Kirschbaum
 */
public interface ArchetypeAttributeVisitor {

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeAnimationName archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeBitmask archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeBool archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeBoolSpec archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeFaceName archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeFixed archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeFloat archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeInt archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeInvSpell archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeList archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeLong archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeMapPath archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeScriptFile archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeSpell archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeString archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeText archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeTreasure archetypeAttribute);

    /**
     * Visitor callback function.
     * @param archetypeAttribute the visited node
     */
    void visit(@NotNull ArchetypeAttributeZSpell archetypeAttribute);

}
